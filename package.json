{
  "name": "util-tiempo",
  "version": "1.0.44",
  "description": "util-tiempo para calcular tiempos, para obtener la fecha a elegir, con muchas opciones!",
  "author": "Tutitoos",
  "license": "MIT",
  "repository": {
    "url": "git://github.com/tutitoos/util-tiempo.git"
  },
  "keywords": [
    "util-tiempo",
    "time",
    "date",
    "tiempo",
    "fecha",
    "timestamp",
    "ms",
    "dataTime",
    "dataDate",
    "diffDate",
    "formatDate",
    "formatMs"
  ],
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --poll src/index.ts",
    "lint": "npm run lint:rome && npm run lint:eslint",
    "lint:rome": "rome check src",
    "lint:eslint": "eslint --ignore-path .gitignore . --max-warnings 0",
    "lint:install": "npm install -g rome eslint"
  },
  "devDependencies": {
    "@types/estree": "1.0.1",
    "@types/node": "20.4.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-typescript": "0.57.0",
    "eslint-plugin-import": "2.27.5",
    "nodemon": "2.0.22",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  }
}
