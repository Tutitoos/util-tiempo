/* Visit https://aka.ms/tsconfig.json to read more about this file */
{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "allowJs": true,
        "checkJs": true,
        "sourceMap": false,
        "outDir": "./lib",
        "rootDir": "./src",
        "declaration": true,
        "declarationDir": "./types",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
